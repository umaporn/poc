datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Subscription {
  id         Int      @id @default(autoincrement())
  endpoint   String   @unique
  p256dh     String
  auth       String
  createdAt  DateTime @default(now())
}
