// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Subscription {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  endpoint  String   @unique
  auth      String
  p256dh    String
  createdAt DateTime @default(now())
}
